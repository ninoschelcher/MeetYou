{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ]
    }
}